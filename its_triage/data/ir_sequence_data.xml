<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <data noupdate="1">

    <record id="sequence_triage" model="ir.sequence">
      <field name="name">Triage</field>
      <field name="code">triage</field>
      <field name="prefix">TRI/%(range_year)s%(range_month)s%(range_day)s</field>
      <field name="padding">4</field>
      <field name="number_next">1</field>
      <field name="number_increment">1</field>
      <field name="company_id"></field>
    </record>

    <record id="seq_salary_slip" model="ir.sequence">
      <field name="name">Salary Slip</field>
      <field name="code">salary.slip</field>
      <field name="prefix">SLIP/</field>
      <field name="padding">3</field>
    </record>

    <record id="seq_packaging" model="ir.sequence">
      <field name="name">Packaging</field>
      <field name="code">packaging</field>
      <field name="prefix">PACK/</field>
      <field name="padding">3</field>
    </record>

  </data>
</odoo>
